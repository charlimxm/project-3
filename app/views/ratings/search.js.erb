$("#resultList").empty()
<% if @dishes.empty? %>
alert("No records found")
<% else %>
<% @dishes.each do |dish| %>
var $listItem = $("<li>")
var $itemLink = $("<a>").attr("href","/dishes/<%= dish.id %>").text("<%= dish.name %>")
$listItem.append($itemLink)
$("#resultList").append($listItem)
<% end %>
<% end %>
