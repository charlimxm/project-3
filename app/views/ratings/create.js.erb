var string = '<%= @dish_id %>'

if (<%= @invalid %> === true){
    alert("You can only vote once")
}
else if (<%= @invalid %> === false){

  var $ratingPara = $(`#ratingPara${string}`)
  var currentRating = (<%= @rating %> +1).toString()
  $ratingPara.text(`Rating: ${currentRating}`)
  alert("Upvoted!")
}
